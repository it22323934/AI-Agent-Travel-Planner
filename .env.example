# AI-Agent-Travel-Planner Environment Variables
# Copy this file to .env and fill in your actual API keys

# Google APIs
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_PLACES_API_KEY=your_places_api_key_here  # Optional, falls back to GOOGLE_API_KEY
GOOGLE_WEATHER_API_KEY=your_weather_api_key_here  # Optional, falls back to GOOGLE_API_KEY

# Ollama Configuration
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3
OLLAMA_MAX_TOKENS=2048
OLLAMA_TEMPERATURE=0.7

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
